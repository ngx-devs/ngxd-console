{
  "name": "ngxd-console",
  "displayName": "Ngxd Console",
  "description": "Ngxd Console for Visual Studio Code",
  "repository": "git@github.com:ngx-devs/ngxd-console",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.67.0"
  },
  "icon": "./icon.png",
  "publisher": "GleisondeAlmeida",
  "author": "Gleison de Almeida",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:typescript"
  ],
  "keywords": [
    "Angular 2",
    "angular-cli",
    "angular",
    "TypeScript",
    "Templates"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ngxd-console-test.generate-component",
        "title": "Ngxd: Generate Component"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "ngxd-console-test.generate-component",
          "when": "explorerResourceIsFolder",
          "group": "ngxd-console"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "precompile": "npm run update-package-json",
    "update-package-json": "node ./src/scripts/precompile.js",
    "preupdate-package-json": "tsc ./src/scripts/precompile.ts",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@npmcli/package-json": "^2.0.0",
    "@semantic-release/git": "^10.0.1",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "14.x",
    "@types/vscode": "^1.67.0",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.14.0",
    "glob": "^8.0.1",
    "husky": "^8.0.1",
    "mocha": "^9.2.2",
    "semantic-release": "^19.0.2",
    "semantic-release-gitmoji": "^1.4.4",
    "semantic-release-vsce": "^5.0.11",
    "typescript": "^4.6.4"
  }
}
